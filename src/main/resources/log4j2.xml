<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p [%40c{1.}] -- %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="com.eclipse.rdf4j" level="WARN" additivity="false">
            <AppenderRef ref="STDOUT"/>
        </Logger>
        <Logger name="mx.araco.miguel" level="DEBUG" additivity="false">
            <AppenderRef ref="STDOUT"/>
        </Logger>
        <Root level="ERROR">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>